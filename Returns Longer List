#Write your function here
def larger_list(lst1, lst2):
  lst1_len = len(lst1)
  lst2_len = len(lst2)
  if lst1_len > lst2_len:
    return lst1[-1]
  if lst2_len > lst1_len:
    return lst2[-1]
  else:
    return lst1[-1]

#Uncomment the line below when your function is done
print(larger_list([4, 10, 2, 5], [-10, 2, 5, 10]))
